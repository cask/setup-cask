{
  "name": "setup-cask",
  "version": "0.0.1",
  "private": true,
  "description": "Github Action to set up a specific Cask version",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "watch": "ncc build src/main.ts --watch",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conao3/setup-cask.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Naoya Yamashita",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.1",
    "@actions/io": "^1.0.1",
    "@actions/tool-cache": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "@types/semver": "^6.0.0",
    "@zeit/ncc": "^0.20.5",
    "husky": "^3.0.9",
    "jest": "^26.6.1",
    "jest-circus": "^26.6.1",
    "prettier": "^1.17.1",
    "ts-jest": "^26.4.3",
    "typescript": "^3.5.1"
  }
}
